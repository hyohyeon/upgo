package com.upgo.dto;

public class CouponEvent {

	private int cpNo;
	private int cpPrice;
	private String cpText;
	private int cpCondition;
	
	public int getCpNo() {
		return cpNo;
	}

	public void setCpNo(int cpNo) {
		this.cpNo = cpNo;
	}

	public int getCpPrice() {
		return cpPrice;
	}

	public void setCpPrice(int cpPrice) {
		this.cpPrice = cpPrice;
	}


	public String getCpText() {
		return cpText;
	}

	public void setCpText(String cpText) {
		this.cpText = cpText;
	}

	public int getCpCondition() {
		return cpCondition;
	}

	public void setCpCondition(int cpCondition) {
		this.cpCondition = cpCondition;
	}

	


}
